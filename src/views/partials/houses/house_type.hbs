<h4 class="h4-break">Zabudowa</h4>
<p><strong>Typ domu:</strong> {{ house_data.houseType }}</p>
{{#if house_data.buildingTypeLemma}}
    <div style="margin-bottom: 10px;"></div>
    <p style="font-style: italic;">Fragmenty: {{ house_data.buildingTypeLemma }}</p>
{{/if}}


<div class="form-check form-check-inline">
    <input class="form-check-input" type="radio" id="house_type_1" name="houseType" value="1" {{#if (equals house_ans_data.technologyAns 1)}}checked{{/if}} required>
    <label class="form-check-label" for="house_type_1">Bliźniacza</label>
</div>
<div class="form-check form-check-inline">
    <input class="form-check-input" type="radio" id="house_type_2" name="houseType" value="2" {{#if (equals house_ans_data.technologyAns 2)}}checked{{/if}}>
    <label class="form-check-label" for="house_type_2">Szeregowa</label>
</div>
        <div class="form-check form-check-inline">
    <input class="form-check-input" type="radio" id="house_type_3" name="houseType" value="3" {{#if (equals house_ans_data.technologyAns 3)}}checked{{/if}}>
    <label class="form-check-label" for="house_type_3">Wolnostojąca</label>
</div>
<hr class="mt-0 mb-4">
