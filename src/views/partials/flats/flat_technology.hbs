<h4 class="h4-break">Technologia</h4>
<p><strong>Materiał</strong>: {{ flat_data.material }}</p>
<p><strong>Rodzaj budynku:</strong> {{  flat_data.buildingType }}</p>
<p><strong>Rok budowy</strong>: {{ flat_data.yearBuilt }}</p>

<div class="question">
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="radio" id="technology_1" name="technology" value="1"
               {{#if (equals flat_ans_data.technologyAns 1)}}checked{{/if}} required>
        <label class="form-check-label" for="technology_1">Tradycyjna</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="radio" id="technology_2" name="technology" value="2"
               {{#if (equals flat_ans_data.technologyAns 2)}}checked{{/if}}>
        <label class="form-check-label" for="technology_2">Tradycyjna udoskonalona</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="radio" id="technology_7" name="technology" value="7"
               {{#if (equals flat_ans_data.technologyAns 7)}}checked{{/if}}>
        <label class="form-check-label" for="technology_7">Wielka Płyta</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="radio" id="technology_3" name="technology" value="3"
               {{#if (equals flat_ans_data.technologyAns 3)}}checked{{/if}}>
        <label class="form-check-label" for="technology_3">Monolityczna</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="radio" id="technology_4" name="technology" value="4"
               {{#if (equals flat_ans_data.technologyAns 4)}}checked{{/if}}>
        <label class="form-check-label" for="technology_4">Drewniana</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="radio" id="technology_6" name="technology" value="6"
               {{#if (equals flat_ans_data.technologyAns 6)}}checked{{/if}}>
        <label class="form-check-label" for="technology_6">Szkielet stalowy</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="radio" id="technology_8" name="technology" value="8"
               {{#if (equals flat_ans_data.technologyAns 8)}}checked{{/if}}>
        <label class="form-check-label" for="technology_8">Rama H</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="radio" id="technology_9" name="technology" value="9"
               {{#if (equals flat_ans_data.technologyAns 9)}}checked{{/if}}>
        <label class="form-check-label" for="technology_9">Alternatywna</label>
    </div>
    {{#if (convertToStr flat_gpt_data.technologyGPT)}}
        <p class="p-break">
            <span class="gpt">GPT: {{flat_gpt_data.technologyGPT}}</span>
            <span class="gpt">GPT: {{flat_gpt_data.technology_summary}}</span>
        </p>
    {{/if}}
</div>
<hr class="mt-0 mb-4">
