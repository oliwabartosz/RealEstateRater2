<h4 class="h4-break">Kuchnia</h4>
{{#if flat_data.kitchenType}}
    <p><strong>Typ kuchni</strong>: {{ flat_data.kitchenType}}</p>
{{/if}}

{{#if flat_data.kitchenAnnexDesc}}
    <p>aneks: {{ flat_data.kitchenAnnexDesc}}</p>
{{/if}}

{{#if flat_data.kitchenBrightDesc}}
    <p>widna: {{ flat_data.kitchenBrightDesc}}</p>
{{/if}}

{{#if flat_data.kitchenDarkDesc}}
    <p>ciemna: {{ flat_data.kitchenDarkDesc}}</p>
{{/if}}

{{#if flat_data.kitchenClearanceDesc}}
    <p>prze≈õwit: {{ flat_data.kitchenClearanceDesc}}</p>
{{/if}}

{{#if flat_data.kitchenWindowDesc}}
    <p>z oknem: {{ flat_data.kitchenWindowDesc}}</p>
{{/if}}

{{#if flat_data.kitchenDiningRoomDesc}}
    <p>z jadalni: {{ flat_data.kitchenDiningRoomDesc}}</p>
{{/if}}


<div class="question">
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="radio" id="kitchen_1" name="kitchen" value="1" {{#if (equals flat_ans_data.kitchenAns 1)}}checked{{/if}} required>
        <label class="form-check-label" for="kitchen_1">Ciemna</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="radio" id="kitchen_2" name="kitchen" value="2" {{#if (equals flat_ans_data.kitchenAns 2)}}checked{{/if}}>
        <label class="form-check-label" for="kitchen_2">Widna</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="radio" id="kitchen_3" name="kitchen" value="3" {{#if (equals flat_ans_data.kitchenAns 3)}}checked{{/if}}>
        <label class="form-check-label" for="kitchen_3">Aneks</label>
    </div>
    {{#if (convertToStr flat_gpt_data.kitchenGPT)}}
        <p class="p-break">
            <span class="gpt">GPT: {{flat_gpt_data.kitchenGPT}}</span>
            <span class="gpt">GPT: {{flat_gpt_data.kitchen_summary}}</span>
        </p>
    {{/if}}
</div>
<hr class="mt-0 mb-4">
